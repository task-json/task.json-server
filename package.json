    "jest": "^26.6.3",
    "supertest": "^6.1.4",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@fastify/jwt": "^6.7.1",
    "fastify": "^4.15.0"
  }
